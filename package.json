{
    "name": "shopping-cart",
    "version": "0.0.1",
    "private": true,
    "devDependencies": {
        "axios": "^0.18.0",
        "enzyme": "^3.6.0",
        "enzyme-adapter-react-16": "^1.5.0",
        "eslint": "5.12.0",
        "eslint-config-prettier": "^2.9.0",
        "gh-pages": "^2.0.1",
        "lint-staged": "^7.2.0",
        "react-scripts": "^2.0.2"
    },
    "dependencies": {
        "prop-types": "^15.6.1",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "redux-thunk": "^2.3.0",
        "react-redux": "^6.0.0",
        "redux": "^4.0.1",
        "redux-logger": "^3.0.6"
    },
    "homepage": "http://sumn2u.github.io/react-redux-boilerplate",
    "scripts": {
        "start": "react-scripts start",
        "format": "prettier --write \"**/*.+(js|md|ts|html|css|sass|less|graphql|yml|yaml|scss|json|vue)\"",
        "precommit": "lint-staged",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "test": "react-scripts test --env=node",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "test:coverage": "npm test -- --coverage"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
